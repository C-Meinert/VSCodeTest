{
    "Serilog": {
        "MinimumLevel": {
          "Default": "Debug",
          "Override": {
            "Microsoft": "Information",
            "System": "Warning"
          }
        },
        "WriteTo": [
          { "Name": "Console" },
          { "Name": "File", 
            "Args": { 
              "path": ".\\Logs\\log.log", 
              "rollingInterval": "Day",
              "rollOnFileSizeLimit": true,
              "fileSizeLimitBytes": "51200"
            } }
        ]
    }
}